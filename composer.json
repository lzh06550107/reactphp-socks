{
    "name": "lzhsocket/socks",
    "description": "Async SOCKS proxy connector client and server implementation, tunnel any TCP/IP-based protocol through a SOCKS5 or SOCKS4(a) proxy server, built on top of ReactPHP.",
    "keywords": ["socks"],
    "homepage": "https://blog.liaozhonghao.cn",
    "license": "MIT",
    "authors": [
        {
            "name": "lzh",
            "email": "liao06550107@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Clue\\React\\Socks\\": "src/",
            "lzhsocket\\Socks\\": "lzhsocket/"
        }
    },
    "require": {
        "php": ">=7.2",
        "react/promise": "^2.1 || ^1.2",
        "react/socket": "^1.1",
        "react/http-client": "0.5.9",
        "monolog/monolog": "^1.24"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0 || ^5.7 || ^4.8.35",
        "react/event-loop": "^1.0 || ^0.5 || ^0.4 || ^0.3",
        "clue/connection-manager-extra": "^1.0 || ^0.7",
        "clue/block-react": "^1.1"
    },
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://packagist.laravel-china.org"
        }
    }
}
